{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"final_space": true,
	"version": 3,
	"palette": {
		"blue": "#3b78ff",
		"cyan": "#41b6b6",
		"darkblack": "#000000",
		"gray": "#cccccc",
		"green": "#13a10e",
		"magenta": "#b4009e",
		"orange": "#e65f3e",
		"red": "#9b0f1b",
		"violet": "#8800dd",
		"white": "#ffffff",
		"yellow": "#cfc98a"
	},
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"background": "p:darkblack",
					"foreground": "p:gray",
					"template": "┌",
					"style": "plain",
					"type": "text"
				},
				{
					"background": "p:red",
					"foreground": "p:white",
					"template": "{{ if .Root }} ADMIN {{ end }}",
					"style": "diamond",
					"type": "session"
				},
				{
					"background": "p:magenta",
					"foreground": "p:white",
					"template": " {{ .HostName }} ",
					"style": "diamond",
					"type": "session"
				},
				{
					"background": "p:green",
					"foreground": "p:white",
					"powerline_symbol": "",
					"template": " {{ .UserName }} ",
					"style": "powerline",
					"type": "session"
				},
				{
					"background": "p:blue",
					"foreground": "p:white",
					"powerline_symbol": "",
					"properties": {
						"style": "agnoster_full"
					},
					"style": "powerline",
					"type": "path"
				},
				{
					"foreground_templates": [
						"{{ if or (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
						"p:white"
					],
					"background_templates": [
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:yellow{{ end }}",
						"{{ if gt .Ahead 0 }}p:green{{ end }}",
						"{{ if gt .Behind 0 }}p:red{{ end }}",
						"p:cyan"
					],
					"powerline_symbol": "",
					"branch_icon": "",
					"branch_ahead_icon": "↑",
					"branch_identical_icon": "≡",
					"branch_behind_icon": "↓",
					"branch_gone_icon": "x",
					"properties": {
						"fetch_status": true,
						"source": "cli"
					},
					"style": "powerline",
					"type": "git",
					"template": " {{ .Ref }}{{ if .BranchStatus }} {{ .BranchStatus }} {{ end }}"
				},
				{
					"background": "p:green",
					"foreground": "p:white",
					"powerline_symbol": "",
					"properties": {
						"fetch_status": true,
						"source": "cli"
					},
					"style": "powerline",
					"type": "git",
					"template": "{{ if .Staging.Changed }} +{{ .Staging.Added }} ~{{ .Staging.Modified }} -{{ .Staging.Deleted }} {{ end }}"
				},
				{
					"background": "p:red",
					"foreground": "p:white",
					"powerline_symbol": "",
					"properties": {
						"fetch_status": true,
						"source": "cli"
					},
					"style": "powerline",
					"type": "git",
					"template": "{{ if .Working.Changed }} +{{ .Working.Untracked }} ~{{ .Working.Modified }} -{{ .Working.Deleted }}{{ end }}{{ if gt .Working.Untracked 0 }}!{{ else if .Staging.Changed }}~{{ end }} "
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "right",
			"segments": [
				{
					"leading_diamond": "",
					"foreground": "p:white",
					"background": "p:violet",
					"template": " {{ .FormattedMs }} ",
					"properties": {
						"threshold": 50,
						"style": "austin",
						"always_enabled": true
					},
					"style": "diamond",
					"type": "executiontime"
				},
				{
					"leading_diamond": "",
					"foreground": "p:white",
					"background": "p:blue",
					"properties": {
						"time_format": "01/02/2006"
					},
					"template": " {{ .CurrentDate | date .Format }} ",
					"style": "diamond",
					"type": "time"
				},
				{
					"leading_diamond": "",
					"foreground": "p:white",
					"background": "p:orange",
					"properties": {
						"time_format": "03:04 PM"
					},
					"template": " {{ .CurrentDate | date .Format }} ",
					"style": "diamond",
					"type": "time"
				},
				{
					"leading_diamond": "",
					"foreground": "p:white",
					"background_templates": [
						"{{ if .Error }}p:red{{ end }}",
						"p:green"
					],
					"template": "{{ if .Error }} ! {{ else }} ✓ {{ end }}",
					"properties": {
						"always_enabled": true
					},
					"style": "diamond",
					"type": "status"
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"background": "p:black",
					"foreground": "p:gray",
					"template": "└─[",
					"style": "plain",
					"type": "text"
				}
			]
		}
	]
}
